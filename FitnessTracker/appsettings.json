{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Error",
      "Microsoft.AspNetCore": "Error",
      "System": "Error"
    }
  },

  "AllowedHosts": "*",

  "JwtSettings": {
    "Key": "your_super_secure_secret_key_that_should_be_long_and_random",
    "Issuer": "FitnessTrackerAPI",
    "Audience": "FitnessTrackerClient",
    "AccessTokenExpiryMinutes": 60,
    "RefreshTokenExpiryDays": 5
  }
}
